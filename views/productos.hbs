  <div class="Container">
    <div class="row">
     {{#each productos}}   
   <div class="col-lg 3 col-md-3 col-sm-12 col-xs-12">
 <div class="card mx-auto m-2" style="width:18rem;" >
      <img src="{{this.foto}}"  class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title"> {{this.nombre}}</h5>
        <p>Carrito Id: {{this.id}}</p>
         <p>Timestamp: {{this.timestamp}}</p> 
        <p class="card-text">Descripcion: {{this.descripcion}}</p>
        <p class="card-text">CÃ³digo:{{this.codigo}}</p>
        <p class="card-text">Precio:{{this.precio}}</p>
        <p class="card-text">Stock:{{this.stock}}</p>
        <button class="btn btn-warning" onclick="agregarProducto('{{this.id}}')">Agregar</button>  
              
      </div>    
    </div>  
     </div>  
  {{else}}  
        <h3 class="alert alert-warning">No se encontraron productos</h3>
    {{/each}}
    </div>
</div>

  
